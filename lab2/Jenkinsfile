/* Requires the Docker Pipeline plugin */
pipeline {
    agent any
    stages {
        stage('build') {
            steps {
		sh '''
		pwd
		whoami
		echo $SHELL
		lab2/test.sh
        python3 -V
        pip3 -V
		python3 lab2/test.py
        . /opt/env/bin/activate
        pip3 install -r lab2/requirements.txt
		'''
            }
        }
        stage('test') {
            steps {
                sh 'echo test'
            }
        }
    }
}
