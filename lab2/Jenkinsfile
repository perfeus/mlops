/* Requires the Docker Pipeline plugin */
pipeline {
    agent any
    stages {
        stage('build') {
            steps {
		sh '''
		pwd
		whoami
		echo $SHELL
		lab2/test.sh
        python3 -V
        pip3 -V
		python3 lab2/test.py
        python3 -m venv /opt/env
		'''
            }
        }
        stage('test') {
            steps {
                sh 'echo test'
            }
        }
    }
}
